              <field>
                <name>FRAME_NUMBER_OVERFLOW</name>
                <description>FrameNumberOverflow Interrupt Enable</description>
                <bitRange>[5:5]</bitRange>
                <enumeratedValues>
                  <enumeratedValue>
                    <name>IGNORE</name>
